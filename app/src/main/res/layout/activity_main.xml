<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="#DDEAF1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My Nano Degree Apps!"
        android:id="@+id/mainMenuTitle"
        android:layout_gravity="center"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="62dp"
        android:layout_marginStart="62dp"
        android:textSize="20dp"
        android:textColor="#000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SPOTIFY STREAMER"
        android:id="@+id/spotifyStreamerLauncherButton"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/mainMenuTitle"
        android:textColor="#000000"
        android:background="#F08C35"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_alignLeft="@+id/scoresAppLauncherButton"
        android:layout_alignStart="@+id/scoresAppLauncherButton"
        android:layout_alignRight="@+id/scoresAppLauncherButton"
        android:layout_alignEnd="@+id/scoresAppLauncherButton"
        android:onClick="sendMessageFromButton"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SCORES APP"
        android:id="@+id/scoresAppLauncherButton"
        android:textColor="#000000"
        android:background="#F08C35"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_below="@+id/spotifyStreamerLauncherButton"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/libraryAppLauncherButton"
        android:layout_alignStart="@+id/libraryAppLauncherButton"
        android:layout_alignRight="@+id/libraryAppLauncherButton"
        android:layout_alignEnd="@+id/libraryAppLauncherButton"
        android:onClick="sendMessageFromButton2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LIBRARY APP"
        android:id="@+id/libraryAppLauncherButton"
        android:textColor="#000000"
        android:background="#F08C35"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_below="@+id/scoresAppLauncherButton"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/buidItBiggerLauncherButton"
        android:layout_alignStart="@+id/buidItBiggerLauncherButton"
        android:layout_alignRight="@+id/buidItBiggerLauncherButton"
        android:layout_alignEnd="@+id/buidItBiggerLauncherButton"
        android:onClick="sendMessageFromButton3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BUILD IT BIGGER"
        android:id="@+id/buidItBiggerLauncherButton"
        android:textColor="#000000"
        android:background="#F08C35"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/libraryAppLauncherButton"
        android:layout_alignLeft="@+id/xyzReaderLauncherButton"
        android:layout_alignStart="@+id/xyzReaderLauncherButton"
        android:layout_alignRight="@+id/xyzReaderLauncherButton"
        android:layout_alignEnd="@+id/xyzReaderLauncherButton"
        android:onClick="sendMessageFromButton4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="XYZ READER"
        android:id="@+id/xyzReaderLauncherButton"
        android:textColor="#000000"
        android:background="#F08C35"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_below="@+id/buidItBiggerLauncherButton"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/capstoneMyOwnAppLauncherButton"
        android:layout_alignStart="@+id/capstoneMyOwnAppLauncherButton"
        android:layout_alignRight="@+id/capstoneMyOwnAppLauncherButton"
        android:layout_alignEnd="@+id/capstoneMyOwnAppLauncherButton"
        android:onClick="sendMessageFromButton5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CAPSTONE: MY OWN APP"
        android:id="@+id/capstoneMyOwnAppLauncherButton"
        android:textColor="#FFFFFF"
        android:background="#800000"
        android:paddingLeft="5dp"
        android:paddingRight="10dp"
        android:layout_below="@+id/xyzReaderLauncherButton"
        android:layout_marginTop="10dp"
        android:layout_alignRight="@+id/mainMenuTitle"
        android:layout_alignEnd="@+id/mainMenuTitle"
        android:layout_alignLeft="@+id/mainMenuTitle"
        android:layout_alignStart="@+id/mainMenuTitle"
        android:onClick="sendMessageFromButton6" />

</RelativeLayout>
